 <% if(users && users.length > 0){ console.log(users) %>
    <table class="table table-striped table-light">
    <thead>
    <tr>
      <th scope="col">Sr No.</th>
      <th scope="col">User ID</th>
      <th scope="col">Username</th>
      <th scope="col">Email Id</th>
      <th scope="col">Age</th>
      <th scope="col">Role</th>
      <th scope="col">Edit</th>
      <th scope="col">Delete</th>
    </tr>
  </thead>
 
  <tbody>
    
      
    <% users.forEach((user, index) => { %>
      <tr>
        <th scope="row"><%= index + 1 %></th>
        <td><%= user._id %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.age %></td>
        <td><%= user.role %></td>
        <td>
          <!-- <form method="POST" action="/edit/<%= user._id %>?_method=PATCH">
            <button class="btn btn-outline-warning">
                <img src="edit.svg" alt="Click" style="height:40px;">
            </button>
          </form> -->
          <a  href="/edit/<%= user._id %>" class="btn btn-outline-warning">
            <img src="/edit.svg" alt="Click" style="height:40px;">
          </a>
          
        </td>
        <td>
          <form method="POST" action="/delete/<%= user._id %>?_method=DELETE">
            <button class="btn btn-outline-danger">
                <img src="/delete.svg" alt="Click" style="height:40px;">
            </button>
          </form>
          
        </td>
      </tr>
    <% }) %>
    <% } else { %>
      <!-- If no users are found, display a message -->
      <div class="alert alert-danger" role="alert">
         No users found!
      </div>
     <% } %> 

    

</tbody>
    </table>